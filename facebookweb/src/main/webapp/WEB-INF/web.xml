<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>FacebookRegistration</servlet-name>
  	<display-name>FacebookRegistration</display-name>
  	<description></description>
  	<servlet-class>com.facebookweb.controller.FacebookRegistration</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.facebookweb.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TimeLineServlet</servlet-name>
  	<display-name>TimeLineServlet</display-name>
  	<description></description>
  	<servlet-class>com.facebookweb.controller.TimeLineServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>FacebookRegistration</servlet-name>
  	<url-pattern>/FacebookRegistration</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TimeLineServlet</servlet-name>
  	<url-pattern>/TimeLineServlet</url-pattern>
  </servlet-mapping>
</web-app>
